msgstr "m/d"

#: inc/class-template.php:72
msgid "Currently"
msgstr "Dabar"

#: inc/class-template.php:73
#, php-format
msgid "High %d&deg;"
msgstr "Aukščiausia %d&deg;"

#: inc/class-template.php:74
#, php-format
